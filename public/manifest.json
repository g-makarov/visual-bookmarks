{
  "name": "New Tab with Visual Bookmarks",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Replacing a new tab with visualized bookmarks.",
  "permissions": ["bookmarks"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone"
}
